@article{ ISI:000282683000032,
Author = {Koprivova, Anna and des Francs-Small, Catherine Colas and Calder, Grant
   and Mugford, Sam T. and Tanz, Sandra and Lee, Bok-Rye and Zechmann,
   Bernd and Small, Ian and Kopriva, Stanislav},
Title = {{Identification of a Pentatricopeptide Repeat Protein Implicated in
   Splicing of Intron 1 of Mitochondrial nad7 Transcripts}},
Journal = {{JOURNAL OF BIOLOGICAL CHEMISTRY}},
Year = {{2010}},
Volume = {{285}},
Number = {{42}},
Pages = {{32192-32199}},
Month = {{OCT 15}},
Abstract = {{Splicing of plant organellar transcripts is facilitated by members of a
   large protein family, the pentatricopeptide repeat proteins. We have
   identified a pentatricopeptide repeat protein in a genetic screen for
   mutants resistant to inhibition of root growth by buthionine sulfoximine
   (BSO), an inhibitor of glutathione synthesis and consequently named BIR6
   (BSO-insensitive roots 6). BIR6 is involved in splicing of intron 1 of
   the mitochondrial nad7 transcript. Loss-of-function mutations in BIR6
   result in a strongly reduced accumulation of fully processed nad7
   transcript. This affects assembly of Complex I and results in moderate
   growth retardation. In agreement with disruption of Complex I function,
   the genes encoding alternative NADH oxidizing enzymes are induced in the
   mutant, and the mutant plants are less sensitive to mannitol and salt
   stress. Mutation in the BIR6 gene allowed normal root growth in presence
   of BSO and strongly attenuated depletion of glutathione content at these
   conditions. The same phenotype was observed with other mutants affected
   in function of Complex I, thus reinforcing the importance of Complex I
   function for cellular redox homeostasis.}},
